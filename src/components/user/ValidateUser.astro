---
import { validateCookie } from "@/utils/validate";
import UserPage from "@/components/user/UserPage";
import { API_URL } from "astro:env/server";

const data = validateCookie(Astro.request.headers);
const url = `${API_URL}/store/UserProducts/${data.Loged.id}`;
const userData = await fetch(url).then((res) => res.json());
---

<UserPage client:load data={userData} />
